{
  "compilerOptions": {
    "target": "ES2020",
    "module": "Commonjs",
    "rootDir": "src",
    "outDir": "dist",
    "strict": true,
    "esModuleInterop": true,
    "types": ["node"],
    "skipLibCheck": true
  },
  "include": ["src/**/*.ts"],
  "exclude": ["node_modules", "dist", "prisma", "prisma.config.ts"]
}
